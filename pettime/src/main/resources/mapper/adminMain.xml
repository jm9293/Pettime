<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.beans.AdminMainDAO">
	<select id="selectRes" flushCache="true" resultType="int">
		SELECT COUNT(*) FROM PETTIME.RESLIST WHERE TO_CHAR(STIME,'YYYYMMDD') = TO_CHAR(SYSDATE,'YYYYMMDD')
	</select>
	
	<select id="selectUser" flushCache="true" resultType="int">
		SELECT COUNT(*) FROM PETTIME.USERDATA
	</select>
</mapper>