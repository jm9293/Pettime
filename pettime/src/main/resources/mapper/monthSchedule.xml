<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqld.pettime.dao.MonthScheduleDAO">

	<select id="select" resultType="com.sqld.pettime.dto.MonthScheduleDTO">
		SELECT NUM, DEGID, HOLIDAY
		FROM MONTHSCHEDULE
		ORDER BY NUM DESC
	</select>
</mapper>